teString();\n  };\n  return (\n    <div>\n      <Background>\n        <ModalWrapper>\n          <ModalCotent>\n            <div className=\"listProjects\">\n              <h3>All Projects</h3>\n              {!select.projects ? (\n                <ClipLoader size=\"30px\" css={override} />\n              ) : !select.projects.projects ? (\n                <ClipLoader size=\"30px\" css={override} />\n              ) : !select.projects.projects.projects ? (\n                <ClipLoader size=\"30px\" css={override} />\n              ) : select.projects.projects.projects < 1 ? (\n                <h3> No projects found</h3>\n              ) : (\n                select.projects.projects.projects.map((e) => (\n                  <div key={e.projects_id} className=\"project_list\">\n                    <ul>\n                      <li>\n                        {e.projects_name}\n                        <span>{getDate(e.date_created)}</span>\n                        <img\n                          src={Bin}\n                          alt=\"Bin\"\n                          onClick={() => {\n                            setState((prevState) => ({\n                              ...prevState,\n                              showDeleteModal: !state.showDeleteModal,\n                              id: e.projects_id,\n                            }));\n                          }}\n                        />\n                      </li>\n                    </ul>\n                  </div>\n                ))\n              )}\n            </div>\n            {state.showDeleteModal ? (\n              <DeleteModal\n                params={state}\n                showModal={setState}\n                action={deleteProject}\n              />\n            ) : null}\n          </ModalCotent>\n          <CloseModal\n            onClick={() => {\n              props.setState((prevState) => ({\n                ...prevState,\n                viewProjects: false,\n              }));\n            }}\