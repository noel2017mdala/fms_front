user inputs\n  and dispatches an action;\n  */\n  const submitInput = (e) => {\n    e.preventDefault();\n    if (state.email === \"\" || state.password === \"\") {\n      paramClass.alertErroMsg(\"Please enter your Email and Password\");\n    } else if (state.email && state.password === \"\") {\n      paramClass.alertErroMsg(\"Please enter details\");\n    } else {\n      dispatch(login(state));\n\n      setState((prevState) => ({\n        ...prevState,\n        loadState: true,\n      }));\n    }\n  };\n\n  const hundleFormSubmit = (e) => {\n    if (e === undefined) {\n      state.loadState = false;\n    } else {\n      cookies.set(\"login\", e, {\n        sameSite: \"strict\",\n        path: \"/\",\n        expires: new Date(new Date().getTime() + 200000 * 10000),\n        // httpOnly: true,\n        // secure: true,\n      });\n\n      if (cookies.get(\"login\")) {\n        history.push(\"/dashboard\");\n      }\n    }\n  };\n  return (\n    <Mainbar>\n      <Router>\n        <div>\n          {cookies.get(\"login\") ? history.push(\"/dashboard\") : \"\"}\n          <p className=\"first\">\n            Find out why <span>Countfy</span> is the Leading Financial\n            management software and why we receive more that\n            <span>12 million</span> Visitors in a single day\n          </p>\n          <p className=\"second\">\n            Countfy Makes tracking and managing of your finances easy, fast and\n            secure. So let's get started now.\n          </p>\n\n          <input\n            className=\"email\"\n            type=\"text\"\n            name=\"email\"\n            placeholder=\"Email\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <input\n            className=\"password\"\n            type=\"password\"\n            name=\"password\"\n            placeholder=\"password\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <button onClick={submitInput}>\n            Let's get started\n            {!state.loadState ? \"\" : \"\"}\n          </button>\n          {!select.login.data ? \"\" : hundleFormSubmit(select.login.data.token)}\n        </div>\n      </Router>\n    </Mainbar>\n  );\n};\n\nexport default Main;\n","/home/abel/fms_front/src/components/Login/Nav.js",[],"/home/abel/fms_front/src/components/Login/Sidebar.js",[],"/home/abel/fms_front/src/components/Login/Login.js",[],"/home/abel/fms_front/src/components/Dashboard/dashboard.js",["160"],"import React from \"react\";\nimport Cookies from \"universal-cookie\";\nimport { BrowserRouter as Router, useHistory } from \"react-router-dom\";\nimport Activities from \"./Activities\";\nimport Project from \"./Projects\";\nimport Sidebar from \"./sideBar\";\nimport UserInfo from \"./userInfo\";\nimport BalanceStats from \"./balanceStats\";\nimport Anouncements from \"./Announcements\";\nimport Nav from \"./Nav\";\nimport { Dashboard as Dash } from \"../../styledComponents/Dashboard/dashboardMain\";\n\nconst style = {\n  display: \"inline\",\n  float: \"left\",\n  margin: \"10px\",\n};\n\nconst Dashboard = () => {\n  const history = useHistory();\n  const cookies = new Cookies();\n\n  return (\n    <Router>\n      {!cookies.get(\"login\") ? history.push(\"/\") : \"\"}\n      <Dash>\n        <Sidebar />\n        <Nav />\n        <UserInfo />\n        <BalanceStats />\n        <Anouncements />\n        <Activities token={cookies.get(\"login\")} />\n        <Project token={cookies.get(\"login\")} />\n      </Dash>\n    </Router>\n  );\n};\n\nexport default Dashboard;\n","/home/abel/fms_front/src/components/Dashboard/Activities.js",[],"/home/abel/fms_front/src/components/Dashboard/options.js",[],"/home/abel/fms_front/src/components/Dashboard/Projects.js",["161"],"/home/abel/fms_front/src/components/Dashboard/sideBar.js",[],"/home/abel/fms_front/src/components/Dashboard/userInfo.js",[],"/home/abel/fms_front/src/components/Dashboard/balanceStats.js",[],"/home/ab