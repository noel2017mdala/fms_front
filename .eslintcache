ponents/Routes.js",[],"/home/abel/fms_front/src/styles/loginStyle.js",[],"/home/abel/fms_front/src/styles/activitiesStyle.js",[],["179","180"],"/home/abel/fms_front/src/redux/reducers/activitiesReducer.js",[],"/home/abel/fms_front/src/styles/options.js",[],"/home/abel/fms_front/src/styles/ProjectsStyle.js",[],"/home/abel/fms_front/src/redux/reducers/projectsReducer.js",[],"/home/abel/fms_front/src/styledComponents/index.js",[],"/home/abel/fms_front/src/components/Login/Footer.js",[],"/home/abel/fms_front/src/components/Login/Main.js",["181","182"],"import { React, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { login } from \"../../redux/actions/actionCreator\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport InfoClass from \"../../classes/componentClass\";\nimport {\n  BrowserRouter as Router,\n  useHistory,\n  Redirect,\n} from \"react-router-dom\";\nimport Cookies from \"universal-cookie\";\nimport { Main as Mainbar } from \"../../styledComponents\";\nconst Main = () => {\n  const [state, setState] = useState({\n    email: \"\",\n    password: \"\",\n    loadState: false,\n  });\n\n  const dispatch = useDispatch();\n  const paramClass = InfoClass;\n  const history = useHistory();\n  const cookies = new Cookies();\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  /*\n  adds user input into a state\n  */\n  const handleSubmit = (e) => {\n    const { name, value } = e.target;\n    setState((prevState) => ({\n      ...prevState,\n      [name]: value,\n      loadState: false,\n    }));\n  };\n\n  /*\n  collects and validates user inputs\n  and dispatches an action;\n  */\n  const submitInput = (e) => {\n    e.preventDefault();\n    if (state.email === \"\" || state.password === \"\") {\n      paramClass.alertErroMsg(\"Please enter your Email and Password\");\n    } else if (state.email && state.password === \"\") {\n      paramClass.alertErroMsg(\"Please enter details\");\n    } else {\n      dispatch(login(state));\n\n      setState((prevState) => ({\n        ...prevState,\n        loadState: true,\n      }));\n    }\n  };\n\n  const hundleFormSubmit = (e) => {\n    if (e === undefined) {\n      state.loadState = false;\n    } else {\n      cookies.set(\"login\", e.token, {\n        sameSite: \"strict\",\n        path: \"/\",\n        expires: new Date(new Date().getTime() + 200000 * 1000),\n        // httpOnly: true,\n        // secure: true,\n      });\n      cookies.set(\"user info\", e.user_info, {\n        sameSite: \"strict\",\n        path: \"/\",\n        expires: new Date(new Date().getTime() + 200000 * 1000),\n        // httpOnly: true,\n        // secure: true,\n      });\n      if (cookies.get(\"login\")) {\n        history.push(\"/dashboard\");\n      }\n    }\n  };\n  return (\n    <Mainbar>\n      <Router>\n        <div>\n          {cookies.get(\"login\") ? history.push(\"/dashboard\") : \"\"}\n          <p className=\"first\">\n            Find out why <span>Countfy</span> is the Leading Financial\n            management software and why we receive more that\n            <span>12 million</span> Visitors in a single day\n          </p>\n          <p className=\"second\">\n            Countfy Makes tracking and managing of your finances easy, fast and\n            secure. So let's get started now.\n          </p>\n\n          <input\n            className=\"email\"\n            type=\"text\"\n            name=\"email\"\n            placeholder=\"Email\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <input\n            className=\"password\"\n            type=\"password\"\n            name=\"password\"\n            placeholder=\"password\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <button onClick={submitInput}>\n            Let's get started\n            {!state.loadState ? \"\" : \"\"}\n          </button>\n          {!select.login.data ? \"\" : hundleFormSubmit(select.login.data)}\n        </div>\n      </Router>\n    </Mainbar>\n  );\n};\n\nexport default Main;\n","/home/abel/fms_front/src/components/Login/Nav.js",[],"/home/abel/fms_front/src/components/Login/Sidebar.js",[],"/home/abel/fms_front/src/components/Login/Login.js",[],"/home/abel/fms_front/src/components/Dashboard/dashboard.js",["183"],"import React from \"react\";\nimport Cookies from \"universal-cookie\";\nimport { BrowserRouter as Router, useHistory } from \"react-router-dom\";\nimport Activities from \"./Activities\";\nimport Project from \"./Projects\";\nimport Sidebar from \"./sideBar\";\nimport UserInfo from \"./userInfo\";\nimport BalanceStats from \"./balanceStats\";\nimport Anouncements from \"./Announcements\";\nimport Nav from \"./Nav\";\nimport { Dashboard as Dash } from \"../../styledComponents/Dashboard/dashboardMain\";\n\nconst style = {\n  display: \"inline\",\n  float: \"left\",\n  margin: \"10px\",\n};\n\nconst Dashboard = () => {\n  const history = useHistory();\n  const cookies = new Cookies();\n\n  return (\n    <Router>\n      {!cookies.get(\"login\") ? history.push(\"/\") : \"\"}\n      <Dash>\n        <Sidebar />\n        <Nav />\n        <UserInfo />\n        <BalanceStats />\n        <Anouncements />\n        <Activities token={cookies.get(\"login\")} />\n        <Project token={cookies.get(\"login\")} />\n      </Dash>\n    </Router>\n  );\n};\n\nexport default Dashboard;\n","/home/abel/fms_front/src/components/Dashboard/Activities.js",[],"/home/abel/fms_front/src/components/Dashboard/Projects.js",["184"],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport { getProjects } from \"../../redux/actions/actionCreator\";\nimport { Projects as Project } from \"../../styledComponents/Dashboard/Projects\";\nimport icon from \"../../images/more-512.webp\";\nconst Projects = (props) => {\n  const dispatch = useDispatch();\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  const getDate = (e) => {\n    let date = new Date(e);\n    return date.toDateString();\n  };\n  useEffect(() => {\n    dispatch(getProjects(props));\n  }, [dispatch, props]);\n  const override = `\n  display: block;\n  margin: 0 auto;\n  border-color: black;\n  margin-top: 10px;\n      `;\n  return (\n    <Project>\n      <div className=\"projectContainer\">\n        <img\n          src={icon}\n          alt=\"options\"\n          onClick={(e) => {\n            console.log(e);\n          }}\n        />\n        <h3>Recent Projects</h3>\n        <div className=\"projectListing\">\n          {!select.projects.data ? (\n            <ClipLoader css={override} size=\"30px\" />\n          ) : !select.projects.data.projects ? (\n            <ClipLoader css={override} size=\"30px\" />\n          ) : select.projects.data.projects < 1 ? (\n            <h3 className=\"info\"> No recent projects found</h3>\n          ) : (\n            select.projects.data.projects.map((e) => (\n              <div key={e.projects_id}>\n                {/* <img\n        src={icon}\n        alt=\"options\"\n        onClick={(e) => {\n          console.log(e);\n        }}\n      /> */}\n                <div className=\"content\">\n                  <p> {e.projects_name}</p>\n                  <span>{getDate(e.date_created)}</span>\n                </div>\n              </div>\n            ))\n          )}\n        </div>\n      </div>\n    </Project>\n  );\n};\n\nexport default Projects;\n","/home/abel/fms_front/src/components/Dashboard/sideBar.js",[],"/home/abel/fms_front/src/components/Dashboard/userInfo.js",[],"/home/abel/fms_front/src/components/Dashboard/balanceStats.js",[],"/home/abel/fms_front/src/components/Dashboard/Announcements.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/dashboardMain.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/dashbordSideBar.js",[],"/home/a