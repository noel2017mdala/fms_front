.modalReducer.projectList ? <ViewProjects /> : null}\n      {select.modalReducer.projectsModal ? <CreateProject /> : null}\n      <Dash>\n        <Nav />\n        <Sidebar />\n        {select.ToggleSidebar ? <DashboardSideBar /> : null}\n        <div className=\"grid_container\">\n          <BalanceStats />\n          <Announcements />\n        </div>\n        <div className=\"grid_Second_container\">\n          <Activities />\n          <Project />\n        </div>\n      </Dash>\n    </Router>\n  );\n};\n\nexport default Dashboard;\n\n/*\n<Sidebar />\n        <Nav />\n        <div className=\"balance_container\">\n          <BalanceStats />\n          <Announcements />\n        </div>\n        <div className=\"activities_container\">\n          <Activities />\n          <Project />\n        </div>\n*/\n","/home/abel/fms_front/src/components/Login/Login.js",["332"],"import React, { Component } from \"react\";\nimport Nav from \"./Nav\";\nimport Main from \"./Main\";\nimport Footer from \"./Footer\";\nimport LoginModal from \"../Dashboard/Modals/loginModal\";\nimport { Container } from \"../../styledComponents\";\nimport { useSelector } from \"react-redux\";\n\nconst Login = () => {\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  return (\n    <Container>\n      <Nav />\n      <Main />\n      <Footer />\n\n      {select.LoginModal.displayModal ? <LoginModal /> : null}\n    </Container>\n  );\n};\n\nexport default Login;\n","/home/abel/fms_front/src/components/Dashboard/sideBar.js",[],"/home/abel/fms_front/src/components/Dashboard/Projects.js",["333","334","335"],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport icon from \"../../images/more-512.webp\";\nimport { getProjects } from \"../../redux/actions/dashboard/projectAction\";\nimport { ToggleProjects } from \"../../redux/actions/dashboard/optionsAction\";\nimport Cookies from \"universal-cookie\";\nimport { Projects as Project } from \"../../styledComponents/Dashboard/Projects\";\nimport ProjectsOption from \"./options/ProjectsOption\";\nimport Common from \"../../classes/componentClass\";\nconst Projects = (props) => {\n  //get redux state\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  //user cookies\n  const cookies = new Cookies();\n  const getToken = cookies.get(\"auth_token\");\n\n  // Dispatch the action when the page loads\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(getProjects(getToken));\n  }, [dispatch]);\n\n  const getDate = (e) => {\n    let date = new Date(e);\n    return date.toDateString();\n  };\n\n  return (\n    <Project>\n      <div className=\"project_container\">\n        <img\n          className=\"projects_image\"\n          src={icon}\n          alt=\"options icon\"\n          onClick={() => {\n            dispatch(ToggleProjects());\n          }}\n        />\n        <h3>Recent Projects</h3>\n      </div>\n      <div className=\"project_items\">\n        {!select.Project ? (\n          <ClipLoader css={Common.loader(\"black\")} size=\"30px\" />\n        ) : !select.Project.projects ? (\n          <ClipLoader css={Common.loader(\"black\")} size=\"30px\" />\n        ) : select.Project.projects < 1 ? (\n          <h3 className=\"info\"> No recent projects found</h3>\n        ) : (\n          select.Project.projects.map((e) => (\n            <div key={e.projects_id} className=\"project_items\">\n              <ul className=\"user_projects\">\n                <li className=\"projectList\">\n                  {e.projects_name}\n                  {/* <span>{getDate(e.date_created)}</span> */}\n                </li>\n              </ul>\n            </div>\n          ))\n        )}\n      </div>\n      {select.ToggleOptions.projects ? <ProjectsOption /> : null}\n    </Project>\n  );\n};\n\nexport default Projects;\n","/home/abel/fms_front/src/components/Dashboard/userInfo.js",[],"/home/abel/fms_front/src/components/Dashboard/Announcements.js",[],"/home/abel/fms_front/src/components/Dashboard/Nav.js",["336"],"import React from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport ReactTooltip from \"react-tooltip\";\nimport { ToggleSidebarAction } from \"../../redux/actions/dashboard/toggleSidebar\";\nimport {\n  Nav as Navigation,\n  LeftSection,\n  RightSection,\n} from \"../../styledComponents/Dashboard/dashboardNav\";\nimport bell from \"../../images/bell.png\";\nimport Messages from \"../../images/mail.png\";\n\nconst Nav = () => {\n  const dispatch = useDispatch();\n  return (\n    <Navigation className=\"nav_bar\">\n      <LeftSection>\n        <div className=\"container\">\n          <label\n            className=\"hubBurger\"\n            onClick={() => {\n              dispatch(ToggleSidebarAction());\n            }}\n          >\n            &#9776;\n          </label>\n          <h4>Dashboard</h4>\n          <p>Easy way to manage your Finances</p>\n        </div>\n      </LeftSection>\n      <RightSection>\n        <div className=\"image-container\">\n          <img src={bell} alt=\"bell\" />\n          <img src={Messages} alt=\"Messages\" />\n        </div>\n      </RightSection>\n    </Navigation>\n  );\n};\n\nexport default Nav;\n","/home/abel/fms_front/src/components/Dashboard/balanceStats.js",["337","338"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Cookies from \"universal-cookie\";\nimport {\n  getBalance,\n  getAmount,\n} from \"../../redux/actions/dashboard/balanceStat\";\nimport { Line } from \"react-chartjs-2\";\nimport { BalanceStats as Statistics } from \"../../styledComponents/Dashboard/BalanceStats\";\n\nconst BalanceStats = () => {\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  const dispatch = useDispatch();\n\n  const cookies = new Cookies();\n  let getUserInfo = cookies.get(\"user_info\")[0];\n  let getToken = cookies.get(\"auth_token\");\n\n  useEffect(() => {\n    dispatch(getBalance(getUserInfo, getToken));\n    dispatch(getAmount(getUserInfo, getToken));\n  }, [dispatch]);\n\n  let days = [];\n  let prevAmount = [];\n  return (\n    <Statistics>\n      {!select.getTransaction.amount_transaction\n        ? \"\"\n        : select.getTransaction.amount_transaction.map((e) => {\n            prevAmount.push(e.prev_Amount);\n            days.push(e.date_value);\n          })}\n      <div className=\"chartContainer\">\n        <p>\n          {select.userBalance.map((e) => {\n            return `Current Balance K ${e.Amount.toLocaleString()}`;\n          })}\n        </p>\n        <Line\n          data={{\n            labels: [...days],\n            datasets: [\n              {\n                label: \"Transaction\",\n                data: [...prevAmount],\n                backgroundColor: [\"rgba(102, 163, 255, 0.6)\"],\n                borderWidth: 1,\n              },\n            ],\n          }}\n          height={100}\n          width={200}\n          options={{\n            responsive: true,\n            maintainAspectRatio: false,\n            scales: {\n              xAxes: [\n                {\n                  gridLines: {\n                    color: \"rgba(0, 0, 0, 0)\",\n                  },\n                },\n              ],\n              yAxes: [\n                {\n                  gridLines: {\n                    color: \"rgba(0, 0, 0, 0)\",\n                  },\n                },\n              ],\n            },\n          }}\n        />\n      </div>\n\n      {/* <button\n        onClick={() => {\n          dispatch(getBalance(getUserInfo, getToken));\n          dispatch(getAmount(getUserInfo, getToken));\n        }}\n      >\n        Text\n      </button> */}\n    </Statistics>\n  );\n};\nexport default BalanceStats;\n","/home/abel/fms_front/src/components/Login/Main.js",[],"/home/abel/fms_front/src/components/Dashboard/Activities.js",["339"],"import React, { useState, useEffect } from \"react\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport Cookies from \"universal-cookie\";\nimport common from \"../../classes/componentClass\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  activitiesAction,\n  getEarnings as earningsData,\n  getExpenses as expenseData,\n} from \"../../redux/actions/dashboard/activities\";\nimport icon from \"../../images/more-512.webp\";\nimport { ToggleOptions } from \"../../redux/actions/dashboard/optionsAction\";\nimport { Activities as Activity } from \"../../styledComponents/Dashboard/Activities\";\nimport Options from \"./options/TransactionOptions\";\nconst Activities = () => {\n  //user tokens\n  const cookies = new Cookies();\n  let getToken = cookies.get(\"auth_token\");\n  let getUserInfo = cookies.get(\"user_info\");\n\n  console.log(getUserInfo);\n\n  //dispatch function\n  const dispatch = useDispatch();\n\n  const userData = {\n    token: getToken,\n    id: getUserInfo,\n  };\n\n  const [state, setState] = useState({\n    active: false,\n    Transations: true,\n    Earnings: false,\n    Expenses: false,\n  });\n\n  //border css\n  const stye = {\n    borderBottom: \"2px solid white\",\n  };\n\n  //call action when page has loaded\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  useEffect(() => {\n    dispatch(activitiesAction(userData));\n  }, [dispatch]);\n\n  const getAllTransacrtions = () => {\n    setState((prevState) => ({\n      ...prevState,\n      Transations: true,\n      Earnings: false,\n      Expenses: false,\n    }));\n    dispatch(activitiesAction(userData));\n  };\n\n  const getEarnings = () => {\n    setState((prevState) => ({\n      ...prevState,\n      Transations: false,\n      Earnings: true,\n      Expenses: false,\n    }));\n    dispatch(earningsData(userData));\n  };\n\n  const getExpenses = () => {\n    setState((prevState) => ({\n      ...prevState,\n      Transations: false,\n      Earnings: false,\n      Expenses: true,\n    }));\n    dispatch(expenseData(userData));\n  };\n\n  return (\n    <Activity>\n      <div className=\"top-bar\">\n        <img\n          className=\"activities_image\"\n          src={icon}\n          alt=\"options icon\"\n          onClick={() => {\n            dispatch(ToggleOptions());\n          }}\n        />\n        <h3>Recent Transactions</h3>\n\n        <ul>\n          <li\n            style={state.Transations ? stye : null}\n            onClick={() => {\n              getAllTransacrtions();\n            }}\n          >\n            Transactions\n          </li>\n          <li\n            style={state.Earnings ? stye : null}\n            onClick={() => {\n              getEarnings();\n            }}\n          >\n            Earnings\n          </li>\n          <li\n            style={state.Expenses ? stye : null}\n            onClick={() => {\n              getExpenses();\n            }}\n          >\n            Expenses\n          </li>\n        </ul>\n      </div>\n      <div className=\"list\">\n        <ul>\n          {!select.getActivities.transaction ? (\n            <ClipLoader css={common.loader(\"black\")} />\n          ) : select.getActivities.transaction.length < 1 ? (\n            <h3>No activities found</h3>\n          ) : (\n            select.getActivities.transaction.map((e) => (\n              <li key={e.id}>\n                {e.transaction_name} K{e.transaction_ammount}\n              </li>\n            ))\n          )}\n        </ul>\n      </div>\n\n      {select.ToggleOptions.transactions ? <Options /> : \"\"}\n    </Activity>\n  );\n};\n\nexport default Activities;\n","/home/abel/fms_front/src/components/Login/Nav.js",["340"],"import { React } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { Header, StyledLink } from \"../../styledComponents\";\nimport { TOGGLE_LOGIN_MODAL } from \"../../redux/actions/dashboard/Modal\";\nimport Image from \"../../images/white_menu.png\";\nimport Sidebar from \"./Sidebar\";\n\nconst Nav = (props) => {\n  const dispatch = useDispatch();\n  return (\n    <Header>\n      <nav>\n        <h1>\n          <a href=\"/\">Countfy</a>\n        </h1>\n        {!props.state ? \"\" : <Sidebar state={props} />}\n\n        <ul className=\"nav-container\">\n          <StyledLink to=\"/\">Home</StyledLink>\n          <StyledLink to=\"/about\">About</StyledLink>\n          <StyledLink to=\"/contacts\">Contacts</StyledLink>\n          <StyledLink\n            onClick={(e) => {\n              e.preventDefault();\n              dispatch(TOGGLE_LOGIN_MODAL());\n            }}\n            to=\"/signup\"\n            className=\"signup\"\n          >\n            Sign up\n          </StyledLink>\n        </ul>\n      </nav>\n      {/* <img\n        src={Image}\n        alt=\"menu\"\n        onClick={() => {\n          props.Slider();\n        }}\n      /> */}\n    </Header>\n  );\n};\n\nexport default Nav;\n","/home/abel/fms_front/src/components/Login/Footer.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/dashboardMain.js",[],"/home/abel/fms_front/src/styledComponents/index.js",["341"],"import styled from \"styled-components\";\nimport { Link } from \"react-router-dom\";\nimport image from \"../images/micheile-henderson-ZVprbBmT8QA-unsplash.jpg\";\n\nconst media = {\n  // mobile_landscape: `@media (max-width: 40em) and (orientation: landscape)`,\n  tablet: `@media(min-width: 40em) and (max-width: 75em)`,\n  bigScreen: `@media (min-width: 50em)`,\n};\n\nexport const Container = styled.div`\n  background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.1)),\n    url(${image});\n  background-size: cover;\n  background-repeat: no-repeat;\n  height: 100vh;\n`;\n\n//navbar links\nexport const StyledLink = styled(Link)`\n  display: inline;\n  color: white;\n  text-decoration: none;\n  margin: 0.5em 1em;\n`;\n\nexport const Header = styled.header`\n  nav {\n    background-color: rgba(0, 0, 0, 0.7);\n    color: white;\n    padding: 1.5em 0;\n  }\n\n  nav h1 {\n    display: inline;\n    margin-left: 1.5em;\n    font-size: 1.5rem;\n    font-weight: normal;\n    text-transform: uppercase;\n    letter-spacing: 0.5em;\n  }\n  nav h1 a {\n    text-decoration: none;\n    color: white;\n    cursor: pointer;\n  }\n\n  nav .nav-container {\n    list-style: none;\n    float: right;\n    margin-right: 2em;\n  }\n  nav .nav-container .signup {\n    border: 1px solid white;\n    padding: 0.5em 0.8em;\n    border-radius: 0.5em;\n  }\n`;\n\n/*\nside bar menu for mobile devices\n*/\nexport const MobileNavContainer = styled.div``;\nexport const MobileNav = styled(Link)``;\n\n/*\nmain content area which contains the login form\nand the sign up page\n*/\nexport const Main = styled.main`\n  .container {\n    max-width: 1080px;\n    margin: 0 auto;\n  }\n\n  .form {\n    display: flex;\n    flex-direction: column;\n    margin-top: 6em;\n  }\n\n  .company-profile {\n    color: white;\n    font-size: 1.8em;\n    text-align: justify;\n  }\n\n  .login-form {\n    margin: 0 auto;\n    margin-top: 3em;\n  }\n\n  .login-form input {\n    padding: 0.8em;\n    margin: 0em 1em;\n  }\n\n  .login-form button {\n    display: block;\n    width: 70%;\n    padding: 1em 1.5em;\n    font-size: 1em;\n    margin: 1.4em auto;\n    border: 1px solid black;\n    border-radius: 0.3em;\n    background-color: black;\n    color: white;\n    cursor: pointer;\n    text-transform: uppercase;\n  }\n`;\n\n//Footer Page\nexport const Footer = styled.footer``;\n","/home/abel/fms_front/src/styledComponents/Dashboard/UserDetails.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/Announcements.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/dashboardNav.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/dashbordSideBar.js",[],"/home/abel/fms_front/src/components/Login/Sidebar.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/BalanceStats.js",[],"/home/abel/fms_front/src/components/Dashboard/options/TransactionOptions.js",["342","343","344","345"],"import React, { useState } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { Transaction } from \"../../../styledComponents/Dashboard/OptionsStyle/Transactions\";\nimport CreateTransaction from \"../Modals/CreateTransactionModal\";\nimport ListTransactions from \"../Modals/listTransactionModal\";\nimport {\n  ToggleActivitiesModals,\n  OPEN_LIST_ActivitiesModals,\n} from \"../../../redux/actions/dashboard/Modal\";\n\n//Options  state lives here\nconst TransactionOptions = (props) => {\n  const dispatch = useDispatch();\n\n  const [state, setState] = useState({\n    showModal: false,\n    ListTransaction: false,\n  });\n\n  /*\n   * This funtion   opens\n   * the create transaction moldal and\n   * closes the list transaction moldal\n   */\n  const showTransactionModal = () => {\n    dispatch(ToggleActivitiesModals());\n  };\n\n  /*\n   * This funtion  opens\n   * the list transaction moldal and\n   * closes the create transaction moldal\n   */\n  const ListTransactionModal = () => {\n    dispatch(OPEN_LIST_ActivitiesModals());\n  };\n\n  return (\n    <Transaction>\n      <div className=\"contain\">\n        <ul className=\"list\">\n          <li\n            onClick={() => {\n              showTransactionModal();\n            }}\n          >\n            Create transaction\n          </li>\n          <li\n            onClick={() => {\n              ListTransactionModal();\n            }}\n          >\n            List all transactions\n          </li>\n        </ul>\n      </div>\n    </Transaction>\n  );\n};\n\nexport default TransactionOptions;\n","/home/abel/fms_front/src/styledComponents/Dashboard/Projects.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/Activities.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/OptionsStyle/Transactions.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/Modals/createTransactionModal.js",[],"/home/abel/fms_front/src/components/Dashboard/Modals/listTransactionModal.js",["346","347"],"import React, { useEffect, useState } from \"react\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  allActivities,\n  activitiesAction,\n} from \"../../../redux/actions/dashboard/activities\";\nimport {\n  getAmount,\n  getBalance,\n} from \"../../../redux/actions/dashboard/balanceStat\";\nimport { CLOSE_LIST_ActivitiesModals } from \"../../../redux/actions/dashboard/Modal\";\nimport Cookies from \"universal-cookie\";\nimport Bin from \"../../../images/bin.png\";\nimport { Container } from \"../../../styledComponents/Dashboard/Modals/listTransactionsModal\";\nimport { deleteActivity as Delete } from \"../../../redux/actions/dashboard/activities\";\nimport common from \"../../../classes/componentClass\";\nconst ListTransactions = ({ state, setState }) => {\n  const cookie = new Cookies();\n  let userInfo = cookie.get(\"user_info\");\n  const token = cookie.get(\"auth_token\");\n  let userData = { token: token, id: userInfo[0].id };\n\n  const dispatch = useDispatch();\n\n  //Redux state selector\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  const [modalState] = useState({\n    showDeleteModal: false,\n    id: \"\",\n    userData: userData,\n  });\n\n  useEffect(() => {\n    dispatch(allActivities(userData));\n  }, [dispatch]);\n\n  const deleteActivity = (data) => {\n    const checkDeleted = dispatch(\n      Delete({\n        ...modalState,\n        id: data,\n      })\n    );\n    if (checkDeleted) {\n      //re-render components\n      dispatch(allActivities(userData));\n      dispatch(getBalance(userInfo[0], token));\n      dispatch(getAmount(userInfo[0], token));\n\n      dispatch(\n        activitiesAction({\n          token: token,\n          id: [userInfo[0]],\n        })\n      );\n    }\n  };\n\n  const getDate = (e) => {\n    let date = new Date(e);\n    return date.toDateString();\n  };\n\n  return (\n    <div>\n      <Container>\n        <div className=\"modalWrapper\">\n          <div className=\"modalItems\">\n            <div className=\"header_container\">\n              <h3 className=\"header\">All Transactions</h3>\n              <div\n                className=\"closeModal\"\n                onClick={(e) => {\n                  e.preventDefault();\n                  dispatch(CLOSE_LIST_ActivitiesModals());\n                }}\n              >\n                &times;\n              </div>\n            </div>\n\n            <div className=\"items\">\n              {!select.ListActivities ? (\n                <ClipLoader css={common.loader(\"black\")} size=\"30px\" />\n              ) : !select.ListActivities.transactions ? (\n                <ClipLoader css={common.loader(\"black\")} size=\"30px\" />\n              ) : select.ListActivities.transactions.length < 1 ? (\n                <h3>No activities found</h3>\n              ) : (\n                select.ListActivities.transactions.map((e) => (\n                  <div key={e.id} className=\"transaction_list\">\n                    <ul>\n                      <li>\n                        {e.transaction_name}\n                        <span className=\"amount\">\n                          K {e.transaction_ammount}\n                        </span>\n                        {/* <span>{getDate(e.transaction_date)}</span> */}\n                        <img\n                          className=\"listTransactionModal_btn\"\n                          src={Bin}\n                          alt=\"Bin\"\n                          onClick={() => {\n                            // console.log(e.id);\n                            deleteActivity(e.id, userData);\n                          }}\n                        />\n                      </li>\n                    </ul>\n                  </div>\n                ))\n              )}\n            </div>\n          </div>\n        </div>\n      </Container>\n    </div>\n  );\n};\n\nexport default ListTransactions;\n","/home/abel/fms_front/src/styledComponents/Dashboard/Modals/listTransactionsModal.js",[],"/home/abel/fms_front/src/components/Dashboard/Modals/DeleteModal.js",[],["348","349"],"/home/abel/fms_front/src/styledComponents/Dashboard/Modals/deleteModal.js",[],"/home/abel/fms_front/src/components/Dashboard/options/ProjectsOption.js",["350","351","352","353","354"],"import React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  Open_Projects_Modal_List,\n  ToggleProjectsModals,\n} from \"../../../redux/actions/dashboard/Modal\";\nimport { Projects } from \"../../../styledComponents/Dashboard/OptionsStyle/Projects\";\nimport CreateProject from \"../Modals/createProjectModal\";\nimport ViewProjects from \"../Modals/ViewProjectsModal\";\n//Options  state lives here\nconst ProjectsOption = (props) => {\n  const [state, setState] = useState({\n    CreateProjectModal: false,\n    viewProjects: false,\n  });\n\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  const dispatch = useDispatch();\n  /*\n  toggles between opening and\n  closing the CreateProjects Projects modal\n  */\n  const CreateProjects = () => {\n    dispatch(ToggleProjectsModals());\n  };\n\n  /*\n  toggles between opening and\n  closing the CreateProjects Projects modal\n  */\n\n  const viewprojects = () => {\n    dispatch(Open_Projects_Modal_List());\n  };\n  return (\n    <Projects>\n      <div className=\"contain\">\n        <ul className=\"list\">\n          <li\n            onClick={() => {\n              CreateProjects();\n            }}\n          >\n            Create Project\n          </li>\n          <li\n            onClick={() => {\n              viewprojects();\n            }}\n          >\n            View all Project\n          </li>\n        </ul>\n      </div>\n    </Projects>\n  );\n};\n\nexport default ProjectsOption;\n","/home/abel/fms_front/src/styledComponents/Dashboard/OptionsStyle/Projects.js",[],["355","356"],"/home/abel/fms_front/src/components/Dashboard/Modals/createProjectModal.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/Modals/createProjectsModal.js",[],"/home/abel/fms_front/src/styledComponents/Dashboard/Modals/viewProjectsModal.js",[],"/home/abel/fms_front/src/components/Dashboard/Modals/ViewProjectsModal.js",["357","358"],"import React, { useEffect, useState } from \"react\";\nimport ClipLoader from \"react-spinners/ClipLoader\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Background } from \"../../../styledComponents/Dashboard/Modals/viewProjectsModal\";\nimport {\n  ListProjects,\n  deleteProject,\n  getProjects,\n} from \"../../../redux/actions/dashboard/projectAction\";\nimport { Close_Projects_Modals_List } from \"../../../redux/actions/dashboard/Modal\";\nimport Cookies from \"universal-cookie\";\nimport Bin from \"../../../images/bin.png\";\n\nconst ViewProjects = (props) => {\n  // user information stored in cookies\n  const cookie = new Cookies();\n  let userInfo = cookie.get(\"user_info\");\n  let userData = { token: cookie.get(\"auth_token\"), id: userInfo[0].id };\n\n  // Action dispatcher function\n  const dispatch = useDispatch();\n\n  //state selector\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  // dispatches the action when the component renders\n  useEffect(() => {\n    dispatch(ListProjects(userData));\n  }, [dispatch]);\n\n  // loader css overider\n  const override = `\n  display: block;\n  margin: 0 auto;\n  border-color: black;\n  margin-top: 10px;\n      `;\n\n  const getDate = (e) => {\n    let date = new Date(e);\n    return date.toDateString();\n  };\n\n  return (\n    <div>\n      <Background>\n        <div className=\"listProjectsWrapper\">\n          <div className=\"listProjects_header\">\n            <h3>All Projects</h3>\n            <div\n              className=\"closeModal\"\n              onClick={(e) => {\n                e.preventDefault();\n                dispatch(Close_Projects_Modals_List());\n              }}\n            >\n              &times;\n            </div>\n\n            <div className=\"items\">\n              {!select.ListProjects ? (\n                <ClipLoader size=\"30px\" css={override} />\n              ) : !select.ListProjects.state ? (\n                <ClipLoader size=\"30px\" css={override} />\n              ) : select.ListProjects.projects < 1 ? (\n                <h3> No projects found</h3>\n              ) : (\n                select.ListProjects.projects.map((e) => (\n                  <div key={e.projects_id} className=\"project_list\">\n                    <ul>\n                      <li>\n                        {e.projects_name}\n                        <span>{getDate(e.date_created)}</span>\n                        <img\n                          src={Bin}\n                          alt=\"Bin\"\n                          className=\"delete_project\"\n                          onClick={() => {\n                            let checkDeleted = dispatch(\n                              deleteProject({\n                                userId: userInfo[0].id,\n                                token: cookie.get(\"auth_token\"),\n                                itemId: e.projects_id,\n                              })\n                            );\n                            if (checkDeleted) {\n                              dispatch(ListProjects(userData));\n                              dispatch(getProjects(cookie.get(\"auth_token\")));\n                            }\n                          }}\n                        />\n                      </li>\n                    </ul>\n                  </div>\n                ))\n              )}\n            </div>\n          </div>\n        </div>\n      </Background>\n    </div>\n  );\n};\n\nexport default ViewProjects;\n","/home/abel/fms_front/src/redux/reducers/getAmountReducer.js",[],"/home/abel