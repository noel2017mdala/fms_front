tory.push(\"/dashboard\");\n      }\n    }\n  };\n  return (\n    <Mainbar>\n      <Router>\n        <div>\n          {cookies.get(\"login\") ? history.push(\"/dashboard\") : \"\"}\n          <p className=\"first\">\n            Find out why <span>Countfy</span> is the Leading Financial\n            management software and why we receive more that\n            <span>12 million</span> Visitors in a single day\n          </p>\n          <p className=\"second\">\n            Countfy Makes tracking and managing of your finances easy, fast and\n            secure. So let's get started now.\n          </p>\n\n          <input\n            className=\"email\"\n            type=\"text\"\n            name=\"email\"\n            placeholder=\"Email\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <input\n            className=\"password\"\n            type=\"password\"\n            name=\"password\"\n            placeholder=\"password\"\n            required=\"required\"\n            onChange={handleSubmit}\n          />\n          <button onClick={submitInput}>\n            Let's get started\n            {!state.loadState ? \"\" : \"\"}\n          </button>\n          {!select.login.data ? \"\" : hundleFormSubmit(select.login.data.token)}\n        </div>\n      </Router>\n    </Mainbar>\n  );\n};\n\nexport default Main;\n","/home/abel/fms_front/src/components/Login/Nav.js",[],"/home/abel/fms_front/src/components/Login/Sidebar.js",[],"/home/abel/fms_front/src/redux/reducers/NavbarReducer.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":1,"column":17,"nodeType":"125","messageId":"126","endLine":1,"endColumn":25},{"ruleId":"123","severity":1,"message":"124","line":1,"column":17,"nodeType":"125","messageId":"126","endLine":1,"endColumn":25},{"ruleId":"119","replacedBy":"127"},{"ruleId":"121","replacedBy":"128"},{"ruleId":"123","severity":1,"message"