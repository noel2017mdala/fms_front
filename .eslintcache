le\";\n\nconst Projects = (props) => {\n  const dispatch = useDispatch();\n  const select = useSelector((e) => {\n    return e;\n  });\n\n  const getDate = (e) => {\n    let date = new Date(e);\n    return date.toDateString();\n  };\n\n  const override = `\n  display: block;\n  margin: 0 auto;\n  border-color: black;\n  margin-top: 120px;\n      `;\n\n  useEffect(() => {\n    dispatch(getProjects(props));\n  }, [dispatch, props]);\n  return (\n    <div style={projectStyle.body}>\n      <h1 style={projectStyle.projects_h1}>Projects</h1>\n      <div>\n        {!select.projects.data ? (\n          <ClipLoader css={override} size=\"40px\" />\n        ) : !select.projects.data.projects ? (\n          <ClipLoader css={override} size=\"40px\" />\n        ) : (\n          select.projects.data.projects.map((e) => (\n            <div key={e.projects_id}>\n              <div style={projectStyle.projects_content}>\n                <p style={projectStyle.projects_p}>\n                  {e.projects_name}\n                  <span style={projectStyle.project_span}>\n                    {getDate(e.date_created)}\n                  </span>\n                </p>\n              </div>\n            </div>\n          ))\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Projects;\n","/home/abel/fms_front/src/styles/ProjectsStyle.js",[],"/home/abel/fms_front/src/components/Login.js",["114"],"import React, { useState } from \"react\";\nimport Nav from \"./Login/Nav\";\nimport Main from \"./Login/Main\";\nimport Footer from \"./Login/Footer\";\nimport { Container } from \"../styledComponents\";\n\nconst Login = () => {\n  return (\n    <Container>\n      <Nav />\n      <Main />\n      <Footer />\n    </Container>\n  );\n};\nexport default Login;\n","/home/abel/fms_front/src/components/Signup.js",[],["115","116"],"/home/abel/fms_front/src/redux/reducers/projectsReducer.js",[],"/home/abel/fms_front/src/styledComponents/index.js",[],"/home/abel/fms_front/src/components/Login/Foo